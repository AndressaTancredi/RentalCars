<% if @subsidiary.errors.full_messages.any? %>
  <p> Não foi possível salvar, verifique os erros abaixo: </p>
  <ul>
  <% @subsidiary.errors.full_messages.each do |m|%>
    <li><%= m %></li>
  <%end%>
  </ul>
<% end %>

<%= form_with(model: @subsidiary, local: true) do |f| %>
  <%= f.label :name %> 
  <%= f.text_field :name %>

  <%= f.label :cnpj %> 
  <%= f.text_field :cnpj %>

  <%= f.label :address %> 
  <%= f.text_field :address %>

  <%= f.submit("Enviar")%>
<% end %>