<% if @subsidiary.errors.full_messages.any? %>
  <p> Não foi possível salvar, verifique os erros abaixo: </p>
  <ul>
  <% @subsidiary.errors.full_messages.each do |m|%>
    <li><%= m %></li>
  <%end%>
  </ul>
<% end %>
<%= form_with(model: @subsidiary) do |f| %>
  <%= f.label :name, "Nome" %> 
  <%= f.text_field :name %>

  <%= f.label :cnpj, "cnpj" %> 
  <%= f.text_field :cnpj %>

  <%= f.label :address, "Address" %> 
  <%= f.text_field :address %>

  <%= f.submit("Enviar")%>
<% end %>